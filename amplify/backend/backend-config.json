        {
          "category": "auth",
          "resourceName": "smile3",
          "attributes": [
            "UserPoolId",
            "AppClientIDWeb",
            "AppClientID"
          ]
        }
      ]
    },
    "smile3": {
      "service": "Cognito",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "function",
          "resourceName": "smile3PostConfirmation",
          "triggerProvider": "Cognito",
          "attributes": [
            "Arn",
            "Name"
          ]
        },
        {
          "category": "function",
          "resourceName": "smile3PreSignup",
          "triggerProvider": "Cognito",
          "attributes": [
            "Arn",
            "Name"
          ]
        }
