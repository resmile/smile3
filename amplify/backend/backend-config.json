      "dependsOn": [
        {
          "category": "auth",
          "resourceName": "smile3",
          "attributes": [
            "UserPoolId"
          ]
        }
      ]
    }
  },
  "api": {
    "AdminQueries": {
      "service": "API Gateway",
      "providerPlugin": "awscloudformation",
      "authorizationType": "AMAZON_COGNITO_USER_POOLS",
